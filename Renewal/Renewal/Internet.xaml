<Window x:Class="Renewal.Internet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Renewal"
        mc:Ignorable="d"
        Title="Internet" Height="1080" Width="240"
        ShowActivated="False" Focusable="False" ShowInTaskbar="False"  FocusManager.IsFocusScope="False" Topmost="True" HorizontalAlignment="Right" HorizontalContentAlignment="Right" VerticalAlignment="Top">

    <WrapPanel x:Name="wrapPanel" Focusable="False" FocusManager.IsFocusScope="False">
        <Button x:Name="Open" Content="Open" Width="240"  Height="180" Click="Open_Click" Focusable="False" FocusManager.IsFocusScope="False"/>
        <Button x:Name="Back" Content="뒤로" Width="240" Height="180" Click="Back_Click" Focusable="False" FocusManager.IsFocusScope="False"/>

    </WrapPanel>
</Window>
