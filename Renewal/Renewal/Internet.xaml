<Window x:Class="Renewal.Internet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Renewal"
        mc:Ignorable="d"
        Title="Internet" Height="1080" Width="240"
        Topmost="True" WindowStyle="None" ShowInTaskbar="False" AllowsTransparency="True">

    <WrapPanel x:Name="wrapPanel" >
        <Button x:Name="Open" Content="Open" Width="240"  Height="180" Click="Open_Click" />
        <Button x:Name="Back" Content="뒤로" Width="240" Height="180" Click="Back_Click" />
        <Button x:Name="Search" Content="검색" Width="240" Height="180" Click="Search_Click" />
        <Button x:Name="Wallpaper" Content="바탕화면" Width="240" Height="180" Click="Wallpaper_Click" />
        <!--Button x:Name="Forward" Content="앞으로" Width="240" Height="180" Click="Forward_Click" Focusable="False" FocusManager.IsFocusScope="False"/-->
        <!--Button x:Name="Re" Content="새로고침" Width="240" Height="180" Click="Re_Click" Focusable="False" FocusManager.IsFocusScope="False"/-->

    </WrapPanel>
</Window>
