<Window x:Class="Renewal.Internet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Renewal"
        mc:Ignorable="d"
        Title="Internet" Height="1080" Width="240"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        ShowActivated="False" Focusable="False" ShowInTaskbar="False"  FocusManager.IsFocusScope="False" Topmost="True" HorizontalAlignment="Right" HorizontalContentAlignment="Right" VerticalAlignment="Top"
        WindowStyle="None"  AllowsTransparency="True">

    <WrapPanel x:Name="wrapPanel" >
        <Button x:Name="Open" Content="Open" Width="240"  Height="180" Click="Open_Click" />
        <Button x:Name="Back" Content="뒤로" Width="240" Height="180" Click="Back_Click" />
        <Button x:Name="Search" Content="검색" Width="240" Height="180" Click="Search_Click" />
        <Button x:Name="Wallpaper" Content="바탕화면" Width="240" Height="180" Click="Wallpaper_Click" />
    </WrapPanel>
</Window>
