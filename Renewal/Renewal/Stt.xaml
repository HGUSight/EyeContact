<Window x:Class="Renewal.Stt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Stt" Height="300" Width="300" Topmost="True">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="235*"/>
            <ColumnDefinition Width="59*"/>
        </Grid.ColumnDefinitions>
        <Button Content="말하기" HorizontalAlignment="Left" Margin="39,31,0,0" VerticalAlignment="Top" Width="75" Click="Start_Click"/>
        <Button Content="말하기 끝" HorizontalAlignment="Left" Margin="39,69,0,0" VerticalAlignment="Top" Width="75" Click="Finish_Click"/>
        <Button Content="확인" HorizontalAlignment="Left" Margin="39,125,0,0" VerticalAlignment="Top" Width="75" Click="OK_Click"/>

    </Grid>
</Window>
