        Title="MainWindow" Height="1080" Width="240">
    
    <WrapPanel x:Name="wrapPanel" >
        <Button x:Name="Mouse" Content="Mouse" Width="240" Height="180" Click="Mouse_Click"/>
        <Button x:Name="Keyboard" Content="Keyboard" Width="240" Height="180" Click="Keyboard_Click"/>
    </WrapPanel>
</Window>
